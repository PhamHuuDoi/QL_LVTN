extends ../../layouts/default.pug

block main
  .d-flex.justify-content-between.align-items-center.mb-3
    h3.mb-0 KẾT QUẢ CUỐI
    a.btn.btn-success(href="/admin/ketqua/export") Xuất Excel

  table.table.table-bordered.table-sm
    thead.thead-dark
      tr
        th Nhóm
        th MSSV
        th Tên SV
        th Lớp
        th Đề tài
        th GVHD
        th Điểm tổng
        th Kết quả
    tbody
      if rows.length
        each r in rows
          tr
            td.text-center= r.group
            td= r.mssv
            td= r.ten
            td= r.lop
            td= r.detai ? r.detai.ten : "—"
            td= r.gvhd ? r.gvhd.hoten : "—"
            td.text-center= r.tong
            td= r.ketqua
      else
        tr
          td(colspan="8", class="text-center text-muted")
            | Chưa có dữ liệu kết quả.
